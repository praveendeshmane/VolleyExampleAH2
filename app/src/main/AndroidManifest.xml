<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="blog.praveendeshmane.co.in.volleyexampleah2">


    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="blog.praveendeshmane.co.in.volleyexampleah2.app.AppController"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="blog.praveendeshmane.co.in.volleyexampleah2.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="blog.praveendeshmane.co.in.volleyexampleah2.ImageRequestActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="blog.praveendeshmane.co.in.volleyexampleah2.JsonRequestActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="blog.praveendeshmane.co.in.volleyexampleah2.StringRequestActivity"
            android:screenOrientation="portrait" >
        </activity>
    </application>

</manifest>